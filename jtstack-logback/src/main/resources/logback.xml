<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="demo2" class="com.jtstack.logback.demo.Demo2">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n
			</pattern>
		</encoder>
	</appender>
	<root level="debug">
		<appender-ref ref="demo2" />
	</root>
</configuration>